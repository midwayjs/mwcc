{
  "name": "@midwayjs/mwcc",
  "version": "0.3.3",
  "description": "Midway Compiler Collection",
  "main": "dist/src/index",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "build": "pegjs src/tsquery/parser.pegjs && node script/version.js && tsc -p . && cpx \"bin/*\" dist/bin",
    "build:test": "cpx \"test/{analyze,cases,comprehension-projects,transformation}/**/*\" dist/test",
    "test": "mocha -r ts-node/register -t 1000000 test/**/*.test.ts",
    "test:js": "mocha -t 1000000 dist/test/**/*.test.js",
    "cov": "nyc --reporter text --reporter lcov mocha -r ts-node/register -t 1000000 test/**/*.test.ts",
    "cov:js": "nyc --reporter text --reporter lcov mocha -t 1000000 dist/test/**/*.test.js"
  },
  "keywords": [
    "midway",
    "compiler",
    "collection"
  ],
  "files": [
    "dist/bin",
    "dist/src"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "cpx": "^1.5.0",
    "globby": "^11.0.0",
    "mocha": "^7.1.1",
    "mwts": "^1.0.6",
    "nyc": "^15.0.0",
    "pegjs": "^0.10.0",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "@midwayjs/ncc": "^0.22.2-1",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.16",
    "typescript": "~3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/midwayjs/mwcc.git"
  }
}
